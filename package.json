  "dependencies": {
    "immer": "7.0.7",
    "rxjs": "6.6.2",
    "rxcomp": "1.0.0-beta.13",
    "tslib": "2.0.0"
  },
  "devDependencies": {
    "@types/node": "14.0.24",
    "gulpfile-config": "1.0.0-alpha.13",
