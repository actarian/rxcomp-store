{
  "author": "Luca Zampetti <lzampetti@gmail.com>",
  "name": "rxcomp-store",
  "description": "Reactive StoreModule for RxComp component library",
  "version": "1.0.0-beta.13",
  "browser": "dist/umd/rxcomp-store.js",
  "main": "dist/cjs/rxcomp-store.js",
