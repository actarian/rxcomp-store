  "dependencies": {
    "immer": "9.0.6",
    "rxjs": "6.6.2",
    "rxcomp": "1.0.0-beta.18",
    "tslib": "2.0.1"
  },
  "devDependencies": {
    "@types/node": "14.6.4",
    "gulpfile-config": "1.0.0-alpha.14",
