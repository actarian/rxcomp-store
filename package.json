    "immer",
    "store",
    "rxjs",
    "es6",
    "es2015"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/actarian/rxcomp-store.git"
  },
  "bugs": {
    "url": "https://github.com/actarian/rxcomp-store/issues"
  },
  "homepage": "https://github.com/actarian/rxcomp-store",
  "scripts": {
    "start": "gulp",
    "build": "gulp build --target dist",
